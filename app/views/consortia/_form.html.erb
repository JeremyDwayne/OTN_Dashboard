<%= form_for @consortium do |f| %>
  <div class="form-group">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :state %><br />
    <%= f.text_field :state, autofocus: true, class: "form-control" %>
  </div>

  <%= f.submit class: "btn btn-primary" %>
<% end %>
